{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Подключить",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceFolder}/server/progs/my_vm",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Включить автоматическое форматирование для gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Задать для варианта приложения дизассемблирования значение Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "processId": 609988
        },
        {
            "name": "Launch server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/server/.",
            "cwd": "${workspaceFolder}/server/progs",
            "args": [],
        },
        {
            "name": "Launch test",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/tests/numConsTest/consTest.go",
            "cwd": "${workspaceFolder}",
            "args": [
                "server/progs/my_asm",
                "server/progs/my_vm",
                "tests/numConsTest/test.code",
                "tests/numConsTest/out"
            ],
        },
        {
            "name": "ASM Launch",
            "type": "cppdbg",
            "request": "launch",
            // Resolved by CMake Tools:
            "program": "${command:cmake.launchTargetPath}",
            "args": [
                "./testdata/test.code",
                "./testdata/out"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "osx": {
                "console": "externalTerminal",
                "MIMode": "lldb"
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "VM Launch",
            "type": "cppdbg",
            "request": "launch",
            // Resolved by CMake Tools:
            "program": "${command:cmake.launchTargetPath}",
            "args": [
                "./testdata/out"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "osx": {
                "externalConsole": true,
                "MIMode": "lldb",
                "internalConsoleOptions": "openOnSessionStart",
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Test Launch",
            "type": "cppdbg",
            "request": "launch",
            // Resolved by CMake Tools:
            "program": "${command:cmake.launchTargetPath}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${command:cmake.launchTargetDirectory}",
            "environment": [],
            "osx": {
                "console": "externalTerminal",
                "MIMode": "lldb"
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
